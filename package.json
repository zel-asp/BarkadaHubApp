  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@supabase/supabase-js": "^2.87.1",
    "@tailwindcss/cli": "^4.1.17",
    "esbuild": "^0.27.1",
    "tailwindcss": "^4.1.17"
  }
}